<h1>Listing favs</h1>

<table>
  <tr>
    <th>Link</th>
    <th>edit</th>
  
    <th>Delete</th>
   
  </tr>

<% @as.each do |fav| %>
  <tr>
    <td><%= link_to fav.name, fav.url ,target: "_blank"%></td>
 
    
    <td><%= link_to 'Edit', edit_fav_path(fav) %></td>
    <td><%= link_to 'Destroy', fav, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Fav', new_fav_path %>
